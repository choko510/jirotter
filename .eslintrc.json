{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-console": "off"
    },
    "globals": {
        "API": "readonly",
        "Utils": "readonly",
        "CheckinComponent": "writable",
        "SearchComponent": "readonly",
        "StampRallyComponent": "readonly",
        "router": "readonly",
        "google": "readonly"
    }
}